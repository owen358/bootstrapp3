<p id="notice"><%= notice %></p>

<h1>Questionnaires</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Q1</th>
      <th>Q2</th>
      <th>Q3</th>
      <th>Q4</th>
      <th>Q5</th>
      <th>Q6</th>
      <th>Q7</th>
      <th>Q8</th>
      <th>Q9</th>
      <th>Q10</th>
      <th>Q11</th>
      <th>Q12</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questionnaires.each do |questionnaire| %>
      <tr>
        <td><%= questionnaire.name %></td>
        <td><%= questionnaire.dob %></td>
        <td><%= questionnaire.q1 %></td>
        <td><%= questionnaire.q2 %></td>
        <td><%= questionnaire.q3 %></td>
        <td><%= questionnaire.q4 %></td>
        <td><%= questionnaire.q5 %></td>
        <td><%= questionnaire.q6 %></td>
        <td><%= questionnaire.q7 %></td>
        <td><%= questionnaire.q8 %></td>
        <td><%= questionnaire.q9 %></td>
        <td><%= questionnaire.q10 %></td>
        <td><%= questionnaire.q11 %></td>
        <td><%= questionnaire.q12 %></td>
        <td><%= link_to 'Show', questionnaire %></td>
        <td><%= link_to 'Edit', edit_questionnaire_path(questionnaire) %></td>
        <td><%= link_to 'Destroy', questionnaire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Questionnaire', new_questionnaire_path %>
